{
  "private": true,
  "scripts": {
    "format": "elm-format src sample/src",
    "start": "elm-doc-preview --port=8110",
    "review": "elm-review --fix",
    "test": "elm-review && elm-format --validate src && elm-verify-examples -r && elm make --docs=/tmp/docs.json"
  },
  "devDependencies": {
    "elm-doc-preview": "^5.0.3",
    "elm-format": "^0.8.3",
    "elm-review": "^2.6.1",
    "elm-verify-examples": "^5.2.0",
    "rimraf": "^2.7.1"
  },
  "dependencies": {
    "elm": "^0.19.1-5"
  }
}
